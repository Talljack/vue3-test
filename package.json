{
  "name": "vue3-music",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "gen-proto": "protoc -I=. src/Api/grpc/mojo.proto --js_out=import_style=commonjs:. --grpc-web_out=import_style=commonjs+dts,mode=grpcwebtext:.",
    "preinstall": "node ./scripts/checkYarn.ts"
  },
  "dependencies": {
    "@commitlint/cli": "^11.0.0",
    "axios": "^0.21.1",
    "better-scroll": "^2.1.3",
    "core-js": "^3.6.5",
    "google-protobuf": "^3.14.0",
    "grpc-web": "^1.2.1",
    "lodash": "^4.17.20",
    "naive-ui": "^1.6.2",
    "nimbus-sdk": "^1.0.0",
    "ts_rpc": "^2.3.5",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "9.0.0-rc.2",
    "vue-router": "^4.0.0-0",
    "vuei18n-translate-loader": "^1.0.5",
    "vuex": "^4.0.0-rc.2",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-nimbus-css-check": "^1.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.11.1",
    "typescript": "~3.9.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{js,ts,vue}": [
      "vue-cli-service lint",
      "vue-cli-service lint:style",
      "git add"
    ],
    "*.{css,scss}": [
      "vue-cli-service lint:style",
      "git add"
    ]
  }
}
